{
    "name": "@ui-schema/dictionary",
    "version": "0.1.0-alpha.3",
    "description": "",
    "homepage": "https://ui-schema.bemit.codes",
    "author": "Michael Becker <michael@bemit.codes> (https://bemit.codes)",
    "license": "MIT",
    "keywords": [
        "ui-schema"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/ui-schema/ui-schema.git",
        "directory": "packages/dictionary"
    },
    "bugs": {
        "url": "https://github.com/ui-schema/ui-schema/issues"
    },
    "scripts": {
        "dtsgen": "tsc --declaration --emitDeclarationOnly --pretty --rootDir ../ && node ../../tools/merge-dirs.js build/dts/dictionary/src build && rimraf build/dts && rimraf --glob build/**/*.test.* build/**/*.mock.*"
    },
    "type": "module",
    "main": "./index.cjs",
    "module": "./esm/index.js",
    "types": "./index.d.ts",
    "exports": {
        ".": {
            "types": "./index.d.ts",
            "import": "./esm/index.js",
            "require": "./index.cjs"
        },
        "./en/*": {
            "types": "./en/*.d.ts",
            "import": "./esm/en/*.js",
            "require": "./en/*.cjs"
        },
        "./de/*": {
            "types": "./de/*.d.ts",
            "import": "./esm/de/*.js",
            "require": "./de/*.cjs"
        },
        "./typeToString": {
            "types": "./typeToString/index.d.ts",
            "import": "./esm/typeToString/index.js",
            "require": "./typeToString/index.cjs"
        },
        "./package.json": "./package.json",
        "./esm": null
    },
    "sideEffects": false,
    "devDependencies": {
        "@ui-schema/json-schema": "~0.5.0-alpha.1",
        "@ui-schema/ui-schema": "~0.5.0-alpha.1"
    },
    "peerDependencies": {
        "@ui-schema/json-schema": "~0.5.0-alpha.1",
        "@ui-schema/ui-schema": "~0.5.0-alpha.1"
    },
    "publishConfig": {
        "tag": "next",
        "access": "public"
    }
}
