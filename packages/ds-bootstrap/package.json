{
    "name": "@ui-schema/ds-bootstrap",
    "version": "0.5.0-alpha.0",
    "description": "Bootstrap Binding for UI-Schema",
    "homepage": "https://ui-schema.bemit.codes",
    "author": "Michael Becker <michael@bemit.codes>",
    "license": "MIT",
    "contributors": [
        "Carolin Holat"
    ],
    "keywords": [
        "ui-schema",
        "json-schema",
        "react",
        "reactjs",
        "react-component",
        "design-system",
        "ui-library",
        "ux",
        "ui",
        "bts",
        "bootstrap"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/ui-schema/ui-schema.git",
        "directory": "packages/ds-bootstrap"
    },
    "bugs": {
        "url": "https://github.com/ui-schema/ui-schema/issues"
    },
    "scripts": {
        "dtsgen": "tsc --declaration --emitDeclarationOnly --pretty --rootDir ../ && node ../../tools/merge-dirs.js build/dts/ds-bootstrap/src build && rimraf build/dts && rimraf --glob build/**/*.test.* build/**/*.mock.*"
    },
    "type": "module",
    "main": "./index.cjs",
    "module": "./esm/index.js",
    "types": "./index.d.ts",
    "exports": {
        ".": {
            "types": "./index.d.ts",
            "import": "./esm/index.js",
            "require": "./index.cjs"
        },
        "./*": {
            "types": "./*/index.d.ts",
            "import": "./esm/*/index.js",
            "require": "./*/index.cjs"
        },
        "./package.json": "./package.json",
        "./esm": null
    },
    "sideEffects": false,
    "devDependencies": {
        "@types/react": "^19.0",
        "@ui-schema/ui-schema": "~0.5.0-alpha.0",
        "@ui-schema/react": "~0.5.0-alpha.0",
        "@ui-schema/json-schema": "~0.5.0-alpha.0",
        "react": "^19.0",
        "react-dom": "^19.0",
        "clsx": "^2.0.0",
        "immutable": "^5.0.0"
    },
    "dependencies": {
    },
    "peerDependencies": {
        "immutable": "^4.0.0 || ^5.0.0",
        "@ui-schema/ui-schema": "~0.5.0-alpha.0",
        "@ui-schema/react": "~0.5.0-alpha.0",
        "@ui-schema/json-schema": "~0.5.0-alpha.0",
        "clsx": "^2.0.0",
        "react": "^18.0 || ^19.0"
    },
    "publishConfig": {
        "tag": "next",
        "access": "public"
    }
}
