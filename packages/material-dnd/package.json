{
    "name": "@ui-schema/material-dnd",
    "version": "0.0.17",
    "description": "Drag and Drop Tools and Widgets for UI-Schema with Material-UI",
    "homepage": "https://ui-schema.bemit.codes",
    "author": "Michael Becker <michael@bemit.codes>",
    "license": "MIT",
    "keywords": [
        "ui-schema",
        "json-schema",
        "mui",
        "material",
        "material-ui",
        "react",
        "reactjs",
        "react-component",
        "form",
        "form-builder"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/ui-schema/ui-schema.git",
        "directory": "packages/material-dnd"
    },
    "bugs": {
        "url": "https://github.com/ui-schema/ui-schema/issues"
    },
    "scripts": {
        "dtsgen": "tsc --declaration --emitDeclarationOnly --pretty --rootDir ../ && node ../../tools/merge-dirs.js build/dts/material-dnd/src build && rimraf build/dts && rimraf --glob build/**/*.test.* build/**/*.mock.*",
        "__release": "cp package.json build/ && cp package-lock.json build/ && cp README.md build/ && npm publish build"
    },
    "main": "./index.cjs",
    "module": "./esm/index.js",
    "types": "./index.d.ts",
    "exports": {
        ".": {
            "types": "./index.d.ts",
            "import": "./esm/index.js",
            "require": "./index.cjs"
        },
        "./*": {
            "types": "./*/index.d.ts",
            "import": "./esm/*/index.js",
            "require": "./*/index.cjs"
        },
        "./esm": null
    },
    "sideEffects": false,
    "dependencies": {
    },
    "devDependencies": {
        "@types/react": "^19.0",
        "@mui/material": "^6.4",
        "@mui/icons-material": "^6.4",
        "@ui-schema/ds-material": "~0.5.0-alpha.0",
        "@ui-schema/kit-dnd": "~0.0.6",
        "@ui-schema/ui-schema": "~0.5.0-alpha.0",
        "@ui-schema/react": "~0.5.0-alpha.0",
        "@ui-schema/json-schema": "~0.5.0-alpha.0",
        "immutable": "^5.0.0",
        "react": "^19.0",
        "react-dnd": "^14.0.0",
        "react-dom": "^19.0"
    },
    "peerDependencies": {
        "@mui/material": "^5.1 || ^6.4",
        "@mui/icons-material": "^5.1 || ^6.4",
        "@ui-schema/ds-material": "~0.5.0-alpha.0",
        "@ui-schema/kit-dnd": "~0.0.6",
        "@ui-schema/ui-schema": "~0.5.0-alpha.0",
        "@ui-schema/react": "~0.5.0-alpha.0",
        "@ui-schema/json-schema": "~0.5.0-alpha.0",
        "immutable": "^4.0.0 || ^5.0.0",
        "react": "^18.0 || ^19.0",
        "react-dnd": "^14.0.0"
    },
    "publishConfig": {
        "tag": "next",
        "access": "public"
    }
}
