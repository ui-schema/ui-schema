        "@ui-schema/kit-dnd": "~0.0.6",
        "@ui-schema/system": "~0.5.0-alpha.0",
        "@ui-schema/react": "~0.5.0-alpha.0",
        "immutable": "^4.0.0",
        "react": "^17.0",
        "react-dnd": "^14.0.0",
        "react-dom": "^18.0"
    },
    "peerDependencies": {
        "@mui/material": "^5.1",
        "@mui/icons-material": "^5.1",
        "@ui-schema/ds-material": "~0.4.0-alpha",
        "@ui-schema/kit-dnd": "~0.0.6",
        "@ui-schema/system": "~0.5.0-alpha.0",
